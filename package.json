    "configuration": {
      "title": "KeybindingMode",
      "properties": {
        "keybindingMode.letterCommandMapping": {
          "type": "array",
          "default": [
            "a,workbench.action.toggleSidebarVisibility"
          ],
          "items": {
            "type": "string",
            "description": "<letter>,<command>"
          },
